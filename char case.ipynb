    "#to find the character is uppercase or lowercase\n",
    "ch=input(\"Enter character: \")\n",
    "if ch.isalpha():\n",
    "    if ch.islower():\n",
    "        print(\"LowerCase\")\n",
    "    else:\n",
    "        print(\"UpperCase\")\n",
    "elif ch.isdigit():\n",
    "    print(\"Numeric\")\n",
    "else:\n",
    "    print(\"Special Character\")"
   ]
